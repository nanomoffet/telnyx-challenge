{
  "name": "telnyx-frontend-starter-kit",
  "version": "0.0.0-development",
  "description": "Boilerplate web application for frontend take-home code challenge",
  "main": "index.js",
  "scripts": {
    "test": "karma start",
    "serve": "webpack-dev-server --open",
    "build": "webpack",
    "api": "json-server --watch api/db.json --port 9001",
    "start": "concurrently --kill-others \"npm run api\" \"npm run serve\"",
    "lint": "tslint -c tslint.json -p tsconfig.json 'src/**/*.ts'",
    "docs": "node_modules/.bin/compodoc -p tsconfig.json -d docs",
    "serve-docs": "node_modules/.bin/compodoc -s -d docs",
    "commitmsg": "validate-commit-msg",
    "cz": "git cz",
    "precommit": "npm run lint",
    "prepush": "npm run test --watch=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/team-telnyx/frontend-starter-kit.git"
  },
  "author": "Telnyx, LLC",
  "license": "UNLICENSED",
  "private": true,
