    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.17",
    "codelyzer": "~4.0.1",
    "concurrently": "^3.5.1",
    "conventional-changelog": "^1.1.6",
    "css-loader": "^0.28.7",
    "cz-conventional-changelog": "^2.0.0",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "install": "^0.10.2",
