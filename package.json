    "@types/webpack": "^3.8.1",
    "@types/webpack-env": "^1.13.2",
    "angular-router-loader": "^0.8.1",
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
